<UserControl x:Class="WPF_Successor_001_to_Vahren.Page026_Battle_SelectUnit"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF_Successor_001_to_Vahren"
      mc:Ignorable="d" 
      Background="AliceBlue"
      d:DesignHeight="60" d:DesignWidth="100"
      Loaded="Page_Loaded">
    <Border x:Name="borPage026" BorderBrush="Black" BorderThickness="4" Height="60" Width="100">
        <Canvas>
            <Canvas.Background>
                <SolidColorBrush Color="Gray" Opacity="0.25" />
            </Canvas.Background>
            <ComboBox x:Name="cmbSelect" Height="52" Width="80" Margin="0,0,0,0"></ComboBox>
            <StackPanel x:Name="stackUnit" Height="52" Margin="80,0,0,0" Orientation="Horizontal"></StackPanel>
        </Canvas>
    </Border>
</UserControl>
